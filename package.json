    "*.{js,jsx,vue,ts,tsx}": [
      "eslint --ext .js,.vue --ignore-path .gitignore --fix src"
    ],
    "*.{js,vue}": "eslint --cache --fix",
    "*.css": "stylelint --fix"
  },
  "husky": {
    "hooks": {
      "*.{js,jsx,vue,ts,tsx}": [
        "yarn lint",
